{
  "expo": {
    "name": "WooCommerce Orders",
    "slug": "woocommerce-orders",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.alimoshen.woocommerceorders"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.alimoshen.woocommerceorders"
    },
    "scheme": "woocommerce-orders",
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "9b7eb5a0-84fa-4546-b245-effec0481231"
      }
    },
    "plugins": ["expo-router", "expo-secure-store"]
  }
}
